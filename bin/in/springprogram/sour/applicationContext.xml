<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="in.springprogram.bean.College" id="colgId">
    
     <property name="collegeName" value="VIPM" />
     <property name="city" value="Berhampur" />
     <property name="pinNo" value="761110" />
     
      </bean>
     
     <bean class="in.springprogram.bean.Student" id="stdId">
    
       <property name="rollNo" value="111" />
     <property name="name" value="Ram" />
     <property name="collg" ref="colgId" />
     
   
    </bean>

</beans>